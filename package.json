{
  "name": "sunnygit",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "homepage": ".",
  "scripts": {
    "start": "cross-env NODE_ENV=development NODE_OPTIONS=--max-old-space-size=4096 BROWSER=none TSC_COMPILE_ON_ERROR=true npm-run-all --parallel start:*",
    "start-built": "npm-run-all start:electron",
    "build": "cross-env GENERATE_SOURCEMAP=false react-scripts build",
    "test": "react-scripts test",
    "package": "electron-builder --dir",
    "package-setup": "electron-builder --publish never",
    "lint": "npm-run-all --parallel lint:*",
    "format": "prettier --write .",
    "clean": "shx rm -rf {build,dist}",
    "pipeline": "npm-run-all clean lint build test package-setup",
    "---- lifecycle ----": "",
    "postinstall": "patch-package && electron-builder install-app-deps",
    "---- internal ----": "",
    "start:cra": "react-scripts start",
    "start:electron": "electron .",
    "lint:eslint": "cross-env ESLINT_IGNORE_TODOS=true eslint src --ext js,jsx,ts,tsx --max-warnings 0",
    "lint:stylelint": "stylelint \"src/**/*.{css,js,jsx,ts,tsx}\" --max-warnings 0",
    "lint:prettier": "prettier --check ."
  },
  "dependencies": {
    "@blueprintjs/core": "^3.35.0",
    "antd": "^4.7.3",
    "crypto-js": "^4.0.0",
    "dayjs": "^1.9.4",
    "lodash": "^4.17.20",
    "monaco-editor": "^0.21.2",
    "nodegit": "^0.27.0",
    "react": "^17.0.1",
    "react-collapse": "^5.0.1",
    "react-dom": "^17.0.1",
    "react-measure": "^2.5.2",
    "react-monaco-editor": "^0.40.0",
    "react-window": "^1.8.5",
    "styled-components": "^5.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^13.1.9",
    "@types/crypto-js": "^4.0.1",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.162",
    "@types/node": "^14.14.2",
    "@types/nodegit": "^0.26.11",
    "@types/react": "^16.9.53",
    "@types/react-collapse": "^5.0.0",
    "@types/react-dom": "^16.9.8",
    "@types/react-measure": "^2.0.6",
    "@types/react-window": "^1.8.2",
    "@types/styled-components": "^5.1.4",
    "cross-env": "^7.0.2",
    "electron": "^10.1.5",
    "electron-builder": "^22.9.1",
    "electron-devtools-installer": "^3.1.1",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "eslint-plugin-sort-destructure-keys": "^1.3.5",
    "monaco-editor-webpack-plugin": "^2.0.0",
    "native-addon-loader": "^2.0.1",
    "node-loader": "^0.6.0",
    "npm-run-all": "^4.1.5",
    "patch-package": "^6.2.2",
    "prettier": "^2.1.2",
    "react-scripts": "4.0.0",
    "shx": "^0.3.2",
    "stylelint": "^13.7.2",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.1.2",
    "type-fest": "^0.18.0",
    "typescript": "^4.0.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "endOfLine": "auto"
  }
}
